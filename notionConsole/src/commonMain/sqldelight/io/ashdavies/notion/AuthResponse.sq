import io.ashdavies.notion.AuthResponse.Owner;

CREATE TABLE authorisation(
    accessToken TEXT PRIMARY KEY NOT NULL,
    workspaceId TEXT NOT NULL,
    workspaceName TEXT,
    workspaceIcon TEXT,
    botId TEXT NOT NULL
);

select:
SELECT * FROM authorisation;

insert:
INSERT INTO authorisation VALUES (?, ?, ?, ?, ?);

deleteAll:
DELETE FROM authorisation;


