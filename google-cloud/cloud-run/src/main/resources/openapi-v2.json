{
  "swagger": "2.0",
  "info": {
    "title": "Playground Service",
    "description": "Playground Service definition hosted by Cloud Endpoints with a Cloud Run backend",
    "version": "1.0.0"
  },
  "host": "${cloud_run_hostname}",
  "schemes": [
    "https"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/hello": {
      "get": {
        "summary": "Greeting",
        "operationId": "hello",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "ApiKeyAuth": {
      "type": "apiKey",
      "name": "X-API-Key",
      "in": "header"
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ],
  "x-google-backend": {
    "address": "${backend_service_name}",
    "protocol": "h2"
  }
}
