{
  "swagger": "2.0",
  "info": {
    "title": "Playground Endpoints",
    "description": "Playground Endpoint definition for Google Cloud Functions",
    "version": "1.0.0"
  },
  "host": "${host}",
  "schemes": [
    "https"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/": {
      "${method}": {
        "summary": "${summary}",
        "operationId": "${operation_id}",
        "x-google-backend": {
          "address": "${address}",
          "protocol": "h2"
        },
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "ApiKeyAuth": {
      "type": "apiKey",
      "name": "X-API-Key",
      "in": "header"
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ]
}
