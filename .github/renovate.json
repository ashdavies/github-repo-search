{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "gitAuthor": "Playground Manager <126197455+playground-manager[bot]@users.noreply.github.com>",
  "packageRules": [
    {
      "matchDatasources": [
        "maven"
      ],
      "registryUrls": [
        "https://maven.google.com"
      ]
    },
    {
      "groupName": "androidx.activity",
      "matchPackagePatterns": [
        "^androidx\\.activity:"
      ]
    },
    {
      "groupName": "androidx.lifecycle",
      "matchPackagePatterns": [
        "^androidx\\.lifecycle:"
      ]
    }
  ],
  "repositories": [
    "ashdavies/playground.ashdavies.dev"
  ],
  "username": "playground-manager[bot]"
}
